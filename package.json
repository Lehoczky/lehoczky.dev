{
  "name": "lehoczky-dev",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "eslint:check": "eslint . --max-warnings=0",
    "eslint:fix": "eslint . --max-warnings=0 --fix",
    "format:check": "prettier . --check --ignore-unknown --cache",
    "format:fix": "prettier . --write --ignore-unknown",
    "knip:check": "knip --config knip.config.ts",
    "lint": "npm run format:check && npm run eslint:check && npm run knip:check",
    "lighthouse:check": "lhci autorun",
    "link:check": "linkinator ./dist --skip www.linkedin.com"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/vue": "^5.1.1",
    "astro": "^5.15.1",
    "clsx": "^2.1.1",
    "reka-ui": "^2.6.0",
    "sal.js": "^0.8.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/bxl": "^1.2.2",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/lucide": "^1.2.70",
    "@lehoczky/eslint-config-vue": "^2.0.11",
    "@lehoczky/prettier-config": "^2.0.2",
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/vite": "^4.1.16",
    "astro-seo-schema": "^5.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-astro": "^1.3.1",
    "knip": "^5.66.2",
    "linkinator": "^7.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "schema-dts": "^1.1.5",
    "sharp": "^0.34.4",
    "unplugin-icons": "^22.5.0"
  },
  "packageManager": "pnpm@10.17.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "oxc-resolver",
      "sharp",
      "vue-demi"
    ]
  }
}
