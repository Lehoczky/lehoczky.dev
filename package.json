{
  "name": "lehoczky-dev",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "eslint:check": "eslint . --max-warnings=0",
    "eslint:fix": "eslint . --max-warnings=0 --fix",
    "format:check": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write --ignore-unknown",
    "knip:check": "knip --config knip.config.ts",
    "lint": "npm run format:check && npm run eslint:check && npm run knip:check",
    "lighthouse:check": "lhci autorun",
    "link:check": "linkinator ./dist --skip www.linkedin.com"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.5",
    "@astrojs/vue": "^5.0.6",
    "astro": "^5.2.6",
    "clsx": "^2.1.1",
    "radix-vue": "^1.9.13",
    "sal.js": "^0.8.5",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/bxl": "^1.2.2",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/lucide": "^1.2.26",
    "@lehoczky/eslint-config-vue": "^2.0.4",
    "@lehoczky/prettier-config": "^2.0.1",
    "@lhci/cli": "^0.14.0",
    "eslint": "^9.20.1",
    "eslint-plugin-astro": "^1.3.1",
    "knip": "^5.44.0",
    "linkinator": "^6.1.2",
    "prettier": "^3.5.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sharp": "^0.33.5",
    "unplugin-icons": "^22.0.0"
  },
  "packageManager": "pnpm@9.15.5"
}
