{
  "name": "lehoczky-dev",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "eslint:check": "eslint . --max-warnings=0",
    "eslint:fix": "eslint . --max-warnings=0 --fix",
    "format:check": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write --ignore-unknown",
    "knip:check": "knip --config knip.config.ts",
    "lint": "npm run format:check && npm run eslint:check && npm run knip:check",
    "lighthouse:check": "lhci autorun",
    "link:check": "linkinator ./dist --skip www.linkedin.com"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.2",
    "@astrojs/vue": "^4.5.2",
    "astro": "^4.16.7",
    "clsx": "^2.1.1",
    "radix-vue": "^1.9.8",
    "sal.js": "^0.8.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@iconify-json/bxl": "^1.2.1",
    "@iconify-json/logos": "^1.2.3",
    "@iconify-json/lucide": "^1.2.11",
    "@lehoczky/eslint-config-vue": "^2.0.3",
    "@lehoczky/prettier-config": "^2.0.0",
    "@lhci/cli": "^0.14.0",
    "eslint": "^9.13.0",
    "eslint-plugin-astro": "^1.3.0",
    "knip": "^5.34.4",
    "linkinator": "^6.1.2",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "sharp": "^0.33.5"
  },
  "packageManager": "pnpm@9.12.3"
}
