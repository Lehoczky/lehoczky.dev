{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "sharedGlobals": [
      "{workspaceRoot}/.node-version",
      "{workspaceRoot}/tsconfig.json"
    ],
    "default": [
      "sharedGlobals",
      "{workspaceRoot}/src/**/*",
      "{workspaceRoot}/public/**/*",
      "{workspaceRoot}/astro.config.mjs",
      "{workspaceRoot}/tailwind.config.ts"
    ],
    "production": ["default"]
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production"],
      "outputs": ["{workspaceRoot}/dist"]
    },
    "lighthouse:check": {
      "cache": true,
      "dependsOn": ["build"],
      "inputs": ["production", "{workspaceRoot}/lighthouserc.cjs"],
      "outputs": ["{workspaceRoot}/.lighthouseci"]
    },
    "eslint:check": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/eslint.config.mjs",
        "{workspaceRoot}/.eslintignore"
      ]
    },
    "format:check": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/.editorconfig",
        "{workspaceRoot}/.prettierrc.mjs",
        "{workspaceRoot}/.prettierignore"
      ]
    },
    "link:check": {
      "cache": true,
      "dependsOn": ["build"],
      "inputs": ["production"]
    }
  },
  "defaultBase": "master",
  "nxCloudAccessToken": "NjJjNzVmNjEtYmNmOC00OTVjLWE1YjQtMjRiYjdhZjVlMzg0fHJlYWQtd3JpdGU="
}
